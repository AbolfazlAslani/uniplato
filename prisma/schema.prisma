// prisma/schema.prisma
generator client {
    provider = "prisma-client-js"
    output   = "../node_modules/.prisma/client"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model Category {
    id        Int     @id @default(autoincrement())
    latitude  Int?
    longitude Int?
    category  String?
    counter   Int?
}
